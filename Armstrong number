public class Main
{
	public static void main(String[] args) {
	   Scanner sc=new Scanner(System.in);
	   int num=sc.nextInt();
	   int count=0;
	   temp=num;
	   while(num>0){
	       count++;
	       num=num/10;
	   }
	   num=temp;
	   int sum=0;
	   while(num>0){
	       int r=num%10;
	       sum=sum+Math.pow(r,count);
	       num=num/10;
	   }
	   if(temp=sum){
	       System.out.println("Amstrong");
	   }else{
	       System.out.println("Not Amstrong");
	   }   
	   }
}
